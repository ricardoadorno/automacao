{
  "behaviors": {
    "fill-order": {
      "actions": [
        {
          "type": "goto",
          "url": "data:text/html,<html><body><h1>Order</h1><label>Pedido</label><input id=\"pedido\" /></body></html>"
        },
        {
          "type": "fill",
          "selector": "#pedido",
          "text": "{pedidoId}"
        },
        {
          "type": "waitForTimeout",
          "ms": 200
        }
      ]
    },
    "swagger-demo": {
      "actions": [
        {
          "type": "goto",
          "url": "data:text/html,<html><body><h1>Swagger Mock</h1><pre id=\"resp\">{\"correlationId\":\"corr-123\",\"id\":\"abc\"}</pre></body></html>"
        }
      ]
    },
    "cloudwatch-demo": {
      "actions": [
        {
          "type": "goto",
          "url": "data:text/html,<html><body><h1>CloudWatch</h1><div id=\"logs\">corr={correlationId}</div></body></html>"
        },
        {
          "type": "waitForSelector",
          "selector": "#logs"
        }
      ]
    }
  }
}
