{
  "metadata": {
    "feature": "order-cloudwatch-before-swagger",
    "ticket": "ORDER-3",
    "env": "local"
  },
  "behaviorsPath": "examples/order/behaviors.json",
  "openapiPath": "examples/order/openapi.json",
  "steps": [
    {
      "id": "cloudwatch",
      "type": "cloudwatch",
      "behaviorId": "cloudwatch-filter-todo",
      "requires": [
        "todoId"
      ]
    },
    {
      "id": "swagger-get",
      "type": "swagger",
      "behaviorId": "swagger-get",
      "config": {
        "operationId": "getTodoById",
        "responseSelector": ".responses-inner"
      }
    }
  ]
}
